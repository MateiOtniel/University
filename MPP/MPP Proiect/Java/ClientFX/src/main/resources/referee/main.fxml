                        <ListView fx:id="activeParticipantsListView" layoutX="28.0" layoutY="34.0" prefHeight="242.0" prefWidth="270.0" />
                        <Label layoutX="28.0" layoutY="14.0" prefHeight="17.0" prefWidth="189.0" text="Participanti activi in proba curenta" />
